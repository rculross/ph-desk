{
  "name": "ph-desk",
  "version": "3.1.274",
  "description": "Advanced desktop application providing intelligent tools and automation for Planhat platform users",
  "main": "electron/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build": "tsc && vite build",
    "build:electron": "npm run build && electron-builder",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:check": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.2",
    "@floating-ui/react": "^0.26.7",
    "@tanstack/react-query": "^5.17.15",
    "@tanstack/react-table": "^8.11.8",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/dompurify": "^3.0.5",
    "antd": "^5.27.3",
    "axios": "^1.11.0",
    "bottleneck": "^2.19.5",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.0",
    "date-fns": "^3.2.0",
    "diff-match-patch": "^1.0.5",
    "dompurify": "^3.2.6",
    "electron-store": "^11.0.0",
    "framer-motion": "^10.18.0",
    "fuse.js": "^7.0.0",
    "loglevel": "^1.9.2",
    "lucide-react": "^0.306.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-hotkeys-hook": "^4.4.4",
    "recharts": "^2.10.3",
    "xlsx": "^0.18.5",
    "xlsx-populate": "^1.21.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/diff-match-patch": "^1.0.36",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.45",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.0.1",
    "electron": "^38.2.1",
    "electron-builder": "^26.0.12",
    "eslint": "^8.55.0",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.4.0",
    "terser": "^5.43.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.1.0",
    "wait-on": "^8.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "electron",
    "desktop-app",
    "planhat",
    "typescript",
    "react",
    "vite"
  ],
  "author": "PH Tools",
  "license": "MIT"
}
